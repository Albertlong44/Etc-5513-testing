filter(Year<= 2021 & Year>=2012,Entity %in% G20) %>%
rename(median_age= "Median.age...Sex..all...Age..all...Variant..estimates") %>% mutate(Entity=(str_remove_all(Entity, " \\(UN\\)")), matcher=paste0(Entity,Code,Year))
